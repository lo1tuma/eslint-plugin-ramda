{
    "ecmaFeatures": {
        "arrowFunctions": true,
        "blockBindings": true,
        "regexUFlag": true,
        "regexYFlag": true,
        "templateStrings": true,
        "binaryLiterals": true,
        "octalLiterals": true,
        "unicodeCodePointEscapes": true,
        "superInFunctions": true,
        "defaultParams": true,
        "restParams": true,
        "forOf": true,
        "objectLiteralComputedProperties": true,
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorthandProperties": true,
        "objectLiteralDuplicateProperties": true,
        "generators": true,
        "destructuring": true,
        "classes": true,
        "spread": true,
        "newTarget": true,
        "modules": true,
        "jsx": false,
        "globalReturn": false,
        "experimentalObjectRestSpread": false
    },
    "env": {
        "builtin": true,
        "browser": true,
        "commonjs": false,
        "node": true,
        "worker": false,
        "amd": false,
        "mocha": false,
        "nashorn": false,
        "jasmine": false,
        "jest": false,
        "phantomjs": false,
        "jquery": false,
        "qunit": false,
        "prototypejs": false,
        "protractor": false,
        "shelljs": false,
        "meteor": false,
        "mongo": false,
        "applescript": false,
        "serviceworker": false,
        "embertest": false,
        "es6": true
    },
    "rules": {
        "complexity": [
            2,
            4
        ],
        "no-inner-declarations": [
            2,
            "functions"
        ],
        "one-var": [
            2,
            {
                "var": "always",
                "let": "never",
                "const": "never"
            }
        ],
        "constructor-super": 2,
        "radix": 2,
        "no-octal": 2,
        "max-len": [
            2,
            120,
            4
        ],
        "no-implied-eval": 2,
        "no-shadow-restricted-names": 2,
        "no-duplicate-case": 2,
        "no-inline-comments": 2,
        "space-before-keywords": [
            2,
            "always"
        ],
        "no-native-reassign": 2,
        "no-param-reassign": [
            2,
            {
                "props": true
            }
        ],
        "require-jsdoc": 0,
        "no-fallthrough": 2,
        "no-extra-boolean-cast": 2,
        "no-self-compare": 2,
        "operator-assignment": [
            2,
            "always"
        ],
        "yoda": [
            2,
            "never"
        ],
        "key-spacing": [
            2,
            {
                "beforeColon": false,
                "afterColon": true
            }
        ],
        "no-alert": 2,
        "no-var": 2,
        "generator-star-spacing": [
            2,
            {
                "before": true,
                "after": false
            }
        ],
        "no-multi-spaces": 2,
        "no-empty-character-class": 2,
        "no-return-assign": 2,
        "no-regex-spaces": 2,
        "accessor-pairs": 0,
        "no-empty-label": 2,
        "no-dupe-args": 2,
        "consistent-this": [
            2,
            "self"
        ],
        "sort-vars": 0,
        "no-invalid-this": 0,
        "max-nested-callbacks": [
            2,
            3
        ],
        "no-implicit-coercion": 2,
        "no-process-env": 2,
        "eol-last": 2,
        "no-plusplus": 2,
        "no-sequences": 2,
        "no-invalid-regexp": 2,
        "no-labels": 2,
        "curly": [
            2,
            "all"
        ],
        "no-extend-native": 2,
        "no-irregular-whitespace": 2,
        "max-depth": [
            2,
            3
        ],
        "space-return-throw-case": 2,
        "no-delete-var": 2,
        "strict": [
            2,
            "global"
        ],
        "no-octal-escape": 2,
        "no-debugger": 2,
        "id-length": 0,
        "quote-props": [
            2,
            "as-needed"
        ],
        "no-floating-decimal": 2,
        "no-undef-init": 2,
        "no-eq-null": 2,
        "no-control-regex": 2,
        "no-restricted-syntax": 0,
        "no-spaced-func": 2,
        "no-new-object": 2,
        "space-in-parens": [
            2,
            "never"
        ],
        "object-shorthand": [
            2,
            "always"
        ],
        "space-after-keywords": [
            2,
            "always"
        ],
        "no-process-exit": 2,
        "no-continue": 2,
        "dot-location": [
            2,
            "property"
        ],
        "no-catch-shadow": 2,
        "no-loop-func": 2,
        "no-array-constructor": 2,
        "no-extra-parens": 2,
        "require-yield": 2,
        "no-negated-in-lhs": 2,
        "no-useless-concat": 0,
        "space-unary-ops": 2,
        "func-style": 0,
        "no-use-before-define": 2,
        "no-useless-call": 2,
        "new-parens": 2,
        "no-shadow": 2,
        "no-mixed-requires": 0,
        "arrow-spacing": [
            2,
            {
                "before": true,
                "after": true
            }
        ],
        "object-curly-spacing": [
            2,
            "always"
        ],
        "prefer-template": 2,
        "no-obj-calls": 2,
        "no-new-func": 2,
        "no-new-require": 2,
        "arrow-parens": 2,
        "no-redeclare": 2,
        "no-script-url": 2,
        "block-scoped-var": 0,
        "no-proto": 2,
        "computed-property-spacing": [
            2,
            "never"
        ],
        "valid-jsdoc": 0,
        "no-constant-condition": 2,
        "no-void": 2,
        "vars-on-top": 2,
        "max-params": [
            2,
            4
        ],
        "no-underscore-dangle": 0,
        "array-bracket-spacing": [
            2,
            "always"
        ],
        "no-restricted-modules": 0,
        "no-throw-literal": 2,
        "block-spacing": 0,
        "no-this-before-super": 2,
        "space-before-blocks": [
            2,
            "always"
        ],
        "no-multiple-empty-lines": [
            2,
            {
                "max": 1
            }
        ],
        "no-extra-bind": 2,
        "prefer-spread": 2,
        "no-sync": 2,
        "space-before-function-paren": [
            2,
            {
                "anonymous": "always",
                "named": "never"
            }
        ],
        "no-dupe-keys": 2,
        "space-infix-ops": 2,
        "max-statements": [
            2,
            10
        ],
        "prefer-reflect": 2,
        "id-match": 0,
        "no-console": 2,
        "no-negated-condition": 0,
        "init-declarations": 0,
        "lines-around-comment": 0,
        "no-unused-expressions": 2,
        "no-lone-blocks": 2,
        "semi": 2,
        "no-caller": 2,
        "wrap-regex": 2,
        "no-ternary": 0,
        "consistent-return": 2,
        "no-warning-comments": [
            2,
            {
                "terms": [
                    "todo",
                    "fixme",
                    "wtf",
                    "falls through",
                    "istanbul"
                ],
                "location": "anywhere"
            }
        ],
        "no-div-regex": 2,
        "no-dupe-class-members": 2,
        "no-extra-semi": 2,
        "no-undefined": 0,
        "no-unreachable": 2,
        "quotes": [
            2,
            "single"
        ],
        "no-ex-assign": 2,
        "wrap-iife": [
            2,
            "outside"
        ],
        "no-label-var": 2,
        "no-with": 2,
        "use-isnan": 2,
        "newline-after-var": [
            2,
            "always"
        ],
        "guard-for-in": 2,
        "no-const-assign": 2,
        "prefer-const": 2,
        "comma-spacing": [
            2,
            {
                "before": false,
                "after": true
            }
        ],
        "no-sparse-arrays": 2,
        "global-require": 0,
        "comma-style": [
            2,
            "last"
        ],
        "no-new": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-else-return": 2,
        "jsx-quotes": 0,
        "spaced-comment": [
            2,
            "always"
        ],
        "brace-style": [
            2,
            "1tbs"
        ],
        "dot-notation": 2,
        "no-trailing-spaces": 2,
        "new-cap": 2,
        "no-new-wrappers": 2,
        "eqeqeq": 2,
        "callback-return": 0,
        "indent": [
            2,
            4,
            {
                "SwitchCase": 0,
                "VariableDeclarator": 1
            }
        ],
        "comma-dangle": [
            2,
            "never"
        ],
        "prefer-arrow-callback": 0,
        "no-unexpected-multiline": 0,
        "no-unused-vars": 2,
        "no-nested-ternary": 2,
        "no-lonely-if": 2,
        "no-multi-str": 2,
        "no-undef": 2,
        "camelcase": 2,
        "no-path-concat": 2,
        "semi-spacing": [
            2,
            {
                "before": false,
                "after": true
            }
        ],
        "no-iterator": 2,
        "no-empty": 2,
        "valid-typeof": 2,
        "func-names": 0,
        "handle-callback-err": [
            2,
            "^(e$|(e|(.*(_e|E)))rr)"
        ],
        "padded-blocks": [
            2,
            "never"
        ],
        "no-cond-assign": 2,
        "no-eval": 2,
        "default-case": 2,
        "linebreak-style": [
            2,
            "unix"
        ],
        "no-unneeded-ternary": 2,
        "no-func-assign": 2,
        "no-class-assign": 2,
        "no-bitwise": 2,
        "operator-linebreak": [
            2,
            "after"
        ]
    }
}
